    </motetype>
    <motetype>
      org.contikios.cooja.mspmote.SkyMoteType
      <identifier>sky4</identifier>
      <description>Sky Mote Type #sky4</description>
      <source EXPORT="discard">[CONTIKI_DIR]/examples/ipv6/rpl-udp-controller-data/udp-malicious-client.c</source>
      <commands EXPORT="discard">make udp-malicious-client.sky TARGET=sky</commands>
